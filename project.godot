; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Desert3_3"
run/main_scene="res://Desert.tscn"
config/icon="res://icon.png"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

fxaa=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/directional_shadow/size=16384
quality/directional_shadow/size.mobile=8192
quality/shadows/filter_mode=2
quality/shadows/filter_mode.mobile=2
quality/filters/anisotropic_filter_level=16
quality/subsurface_scattering/quality=2
quality/voxel_cone_tracing/high_quality=true
quality/filters/msaa=3
quality/filters/use_fxaa=true
quality/filters/use_debanding=true
environment/default_environment="res://default_env.tres"
